    "lint-staged": "^9.2.0",
    "pify": "^4.0.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-intl": "^2.9.0",
    "temp-write": "^4.0.0",
    "tempy": "^0.3.0",
    "xo": "^0.24.0"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "operator-linebreak": 0,
      "indent": 0
    }
  },
  "ava": {
    "helpers": [
      "**/fixtures/**/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }

